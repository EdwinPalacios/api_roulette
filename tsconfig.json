{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6"
    ],
    "types": [
      "reflect-metadata",
      "node",
      "jest"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"],
      "~/*": ["src/*"],
      "@ioc/*": ["src/ioc/*"],
      "@routes/*": ["src/routes/*"],
      "@services/*": ["src/services/*"],
      "@models/*": ["src/models/*"],
      "@utils/*": ["src/utils/*"],
    },
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": []
}